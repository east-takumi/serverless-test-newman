{
  "Comment": "時刻変換と日付計算ワークフロー",
  "StartAt": "ConvertToJST",
  "States": {
    "ConvertToJST": {
      "Type": "Task",
      "Resource": "${ConvertToJSTFunctionArn}",
      "Next": "CalculateTimeDifference"
    },
    "CalculateTimeDifference": {
      "Type": "Task",
      "Resource": "${CalculateTimeDifferenceFunctionArn}",
      "Next": "FormatResults"
    },
    "FormatResults": {
      "Type": "Task",
      "Resource": "${FormatResultsFunctionArn}",
      "End": true
    }
  }
}
